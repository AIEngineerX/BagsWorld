{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": false
  },
  "include": ["server.ts", "src/**/*.ts"],
  "exclude": ["node_modules", ".next"]
}
